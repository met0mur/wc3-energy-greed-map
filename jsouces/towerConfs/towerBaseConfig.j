/*its call vjass @Loading structs from SLK files@*///! loaddata "slk/TowerBaseConfig.slk"library towerBaseConfig uses general, asciiTable {		struct TowerBaseConfig {				string ident;		integer level;		integer distance;		integer capacity;		integer hp;		integer buildCost;		integer damage;		integer attackCost;		integer cooldown;				integer dpscc;		integer mpscc;		integer dpsmps;		integer bcdmg;		integer dpm;		integer bcdps;				string abilid;		integer abilid_int = 0;		string unitid;		integer unitid_int = 0;				integer dmult;		integer dpsmul;		integer atcmul;				string unam;		string utup;		string uhot;		string anam;		string ahky;				string abdmg;		string abprop1;		integer abval1;		string abline1;				private static integer pointer = 1;				static TowerBaseConfig array values        static method getFromKey takes integer i returns TowerBaseConfig			values[i]=TowerBaseConfig.create()			pointer = i+1;			return TowerBaseConfig.values[i]        endmethod				static TowerBaseConfig getByObjectId(integer id) {			integer i = pointer;			whilenot (i-- == 1) {				TowerBaseConfig conf = values[i];				if (conf != 0) {					if (conf.unitid_int == 0) {						conf.unitid_int = ascii.string2int(conf.unitid);					}					if (conf.unitid_int == id) {						return conf;					}					if (conf.abilid_int == 0) {						conf.abilid_int = ascii.string2int(conf.abilid);					}					if (conf.abilid_int == id) {						return conf;					}				}			}			return 0;		}	}}